{
  "name": "twenty-four-words",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-native": "^0.55.28",
    "@types/react-redux": "^6.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux-logger": "^3.0.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "jest-expo": "~31.0.0",
    "react-test-renderer": "16.3.1",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.3",
    "schedule": "^0.4.0",
    "ts-jest": "^23.10.5",
    "tsc-watch": "^1.0.31",
    "tscpaths": "0.0.6",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "eject": "expo eject",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "jest",
    "lint": "tslint src/**/*.ts",
    "tscpaths": "tscpaths -p tsconfig.json -s ./src -o ./build",
    "tsc": "tsc --project tsconfig.json && npm run tscpaths",
    "tsc-watch": "tsc-watch --project tsconfig.json --onSuccess \"npm run tscpaths\"",
    "clean": "rimraf build",
    "build": "npm run clean && npm run tsc",
    "watch": "npm run clean && npm run tsc-watch",
    "watch-run-android": "concurrently \"npm run watch\" \"npm run android\"",
    "go-android": "npm run build && npm run watch-run-android ",
    "watch-run-ios": "concurrently \"npm run watch\" \"npm run ios\"",
    "go-ios": "npm run build && npm run watch-run-ios ",
    "watchAndStart": "concurrently \"npm run watch\" \"npm run start\"",
    "buildAndStart": "npm run build && npm run watchAndStart "
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^31.0.6",
    "firebase": "^5.5.2",
    "react": "^16.7.0",
    "react-native": "0.57.1",
    "react-native-elements": "^1.0.0-beta7",
    "react-native-firebase": "^5.1.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  }
}
