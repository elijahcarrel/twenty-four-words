{
  "name": "twenty-four-words",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "eject": "expo eject",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "jest",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc --project tsconfig.json",
    "tsc-watch": "tsc-watch --project tsconfig.json",
    "clean": "rimraf build",
    "build": "npm run clean && npm run tsc",
    "watch": "npm run clean && npm run tsc-watch",
    "watch-run-android": "concurrently npm:watch npm:android --handle-input --default-input-target 1",
    "go-android": "npm run build && npm run watch-run-android",
    "watch-run-ios": "concurrently npm:watch npm:ios --handle-input --default-input-target 1",
    "go-ios": "npm run build && npm run watch-run-ios",
    "watch-run": "concurrently npm:watch npm:start",
    "go": "npm run build && npm run watchAndStart"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^35.0.1",
    "firebase": "^7.3.0",
    "react": "^16.11.0",
    "react-native": "0.61.4",
    "react-native-elements": "^1.2.7",
    "react-native-firebase": "^5.5.6",
    "react-navigation": "^4.0.10",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-native": "^0.60.22",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.1",
    "@types/redux-logger": "^3.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "jest-expo": "~35.0.0",
    "react-test-renderer": "16.11.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "schedule": "^0.5.0",
    "ts-jest": "^24.1.0",
    "tsc-watch": "^4.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
